{
  "name": "teachteam-admin-backend",
  "version": "1.0.0",
  "description": "TeachTeam Admin Backend with GraphQL - HD Requirements Implementation",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "watch": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:hd": "jest --testNamePattern=\"HD\" --verbose",
    "debug-db": "ts-node src/debug-db.ts",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "teachteam",
    "admin",
    "graphql",
    "typescript",
    "assignment2",
    "hd-requirements"
  ],
  "dependencies": {
    "apollo-server-express": "^3.12.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "graphql": "^15.8.0",
    "mysql2": "^3.6.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/express-session": "^1.17.9",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "Assignment 2 HD Requirements Implementation"
  },
  "author": "TeachTeam Development Team",
  "license": "ISC"
}